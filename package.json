{
	"name": "example-react-app",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"immutable": "^3.8.1",
		"material-ui": "^0.18.3",
		"react": "^15.5.4",
		"react-dom": "^15.5.4",
		"react-redux": "^5.0.5",
		"react-tap-event-plugin": "^2.0.1",
		"redux": "^3.6.0",
		"redux-devtools-extension": "^2.13.2",
		"redux-saga": "^0.15.3",
		"studiokit-auth-js": "^0.0.7",
		"studiokit-net-js": "^0.0.16"
	},
	"devDependencies": {
		"pre-git": "^3.15.0",
		"prettier": "^1.4.4",
		"react-scripts": "^1.0.7"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test --env=jsdom",
		"eject": "react-scripts eject",
		"commit": "commit-wizard"
	},
	"config": {
		"pre-git": {
			"commit-msg": "simple",
			"pre-commit": [
				"yarn prettier -- --print-width 120 --use-tabs --no-semi --single-quote --jsx-bracket-same-line --write 'src/**/*.js'"
			]
		}
	}
}
